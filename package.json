{
  "name": "options-calculator",
  "private": true,
  "scripts": {
    "start:dev": "concurrently \"nodemon\" \"PORT=3000 yarn frontend start\"",
    "start": "node index.js",
    "heroku-postbuild": "cd frontend && yarn && yarn run build",
    "test": "yarn workspaces run test"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js",
    "exec": "yarn start"
  },
  "devDependencies": {
    "concurrently": "^5.2.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "~4.16.1",
    "helmet": "^3.22.0",
    "http-errors": "~1.6.3",
    "http-proxy-middleware": "^1.0.4",
    "http-status-codes": "^1.4.0",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.7"
  }
}
